{
  "name": "tinder",
  "version": "0.0.1",
  "description": "",
  "author": "tsaqifrazin@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/integrations/jest-e2e.json --forceExit --runInBand",
    "typeorm": "npx typeorm-ts-node-commonjs ",
    "typeorm:create-migration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "typeorm:run-migration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/utils/ormconfig migration:run",
    "typeorm:revert-migration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/utils/ormconfig migration:revert",
    "typeorm:generate-migration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/utils/ormconfig migration:generate",
    "typeorm:db:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs db:create -d ./src/utils/ormconfig",
    "typeorm:create-seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "typeorm:run-seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/utils/ormconfigSeed migration:run",
    "typeorm:revert-seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/utils/ormconfigSeed migration:revert",
    "prepare": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@json2csv/plainjs": "^7.0.3",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "9.2.1",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/multer": "^1.4.7",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.9.0",
    "pg-mem": "^2.8.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "0.3.10",
    "typeorm-extension": "^3.5.0",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "^2.1.0",
    "@automock/jest": "^2.1.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "9.2.1",
    "@types/express": "^4.17.13",
    "@types/flat": "^5.0.5",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "28.1.3",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/src/$1",
      "^common/(.*)": "<rootDir>/src/common/$1",
      "^decorators/(.*)": "<rootDir>/src/decorators/$1",
      "^exceptions/(.*)": "<rootDir>/src/exceptions/$1",
      "^filters/(.*)": "<rootDir>/src/filters/$1",
      "^guards/(.*)": "<rootDir>/src/guards/$1",
      "^interceptors/(.*)": "<rootDir>/src/interceptors/$1",
      "^interfaces/(.*)": "<rootDir>/src/interfaces/$1",
      "^middlewares/(.*)": "<rootDir>/src/middlewares/$1",
      "^migrations/(.*)": "<rootDir>/src/migrations/$1",
      "^modules/(.*)": "<rootDir>/src/modules/$1",
      "^utils/(.*)": "<rootDir>/src/utils/$1"
    }
  },
  "lint-staged": {
    "*.{json,ts}": [
      "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "prettier --write \"{src,apps,libs,test}/**/*.ts\""
    ],
    "*.js": "eslint --cache --fix"
  }
}
